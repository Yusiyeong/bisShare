		<typeAlias type="com.bs.address.vo.AddressVo" alias="addressvo"/>
		<!-- 게시판 -->
		<typeAlias type="com.bs.notice.vo.NoticeVo" alias="nv"/>
		<typeAlias type="com.bs.free.vo.FreeVo" alias="fv"/>
		<typeAlias type="com.bs.blind.vo.BlindVo" alias="bv"/>
